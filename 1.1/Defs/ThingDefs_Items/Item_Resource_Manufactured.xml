  <!-- Dark Steel -->
  <ThingDef ParentName="DarkShellBase">
    <defName>DarkSteel</defName>
    <label>Dark Steel</label>
    <description>A metalic material of unequal quality, made from plasteel and dark matter.</description>
    <graphicData>
      <texPath>Items/DarkSteel</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>100</stackLimit>
    <statBases>
      <MaxHitPoints>50000</MaxHitPoints>
      <MarketValue>600</MarketValue>
      <Mass>2</Mass>
      <Flammability>0</Flammability>
      <DeteriorationRate>0</DeteriorationRate>
      <StuffPower_Armor_Sharp>2.5</StuffPower_Armor_Sharp>
      <StuffPower_Armor_Blunt>1.5</StuffPower_Armor_Blunt>
      <StuffPower_Armor_Heat>1.5</StuffPower_Armor_Heat>
      <StuffPower_Insulation_Cold>50</StuffPower_Insulation_Cold>
      <StuffPower_Insulation_Heat>50</StuffPower_Insulation_Heat>
      <SharpDamageMultiplier>2.5</SharpDamageMultiplier>
      <BluntDamageMultiplier>2.5</BluntDamageMultiplier>
    </statBases>
    <intricate>true</intricate>
    <thingCategories>
      <li>Manufactured</li>
      <li>ResourcesRaw</li>
    </thingCategories>
    <tradeTags>
      <li>ExoticMisc</li>
    </tradeTags>
    <stuffProps>
      <categories>
        <li>Metallic</li>
      </categories>
      <commonality>0.01</commonality>
      <constructEffect>ConstructMetal</constructEffect>
      <color>(95,17,145)</color>
      <soundImpactStuff>BulletImpact_Metal</soundImpactStuff>
      <soundMeleeHitSharp>MeleeHit_Metal_Sharp</soundMeleeHitSharp>
      <soundMeleeHitBlunt>MeleeHit_Metal_Blunt</soundMeleeHitBlunt>
      <statFactors>
        <MaxHitPoints>50</MaxHitPoints>
        <Beauty>10</Beauty>
        <Flammability>0</Flammability>
        <WorkToMake>100</WorkToMake>
        <WorkToBuild>100</WorkToBuild>
        <DoorOpenSpeed>4</DoorOpenSpeed>
        <MeleeWeapon_CooldownMultiplier>0.2</MeleeWeapon_CooldownMultiplier>
      </statFactors>
    </stuffProps>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
  </ThingDef>
