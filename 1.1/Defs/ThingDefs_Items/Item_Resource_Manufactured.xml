  <ThingDef ParentName="DarkShellBase">
    <defName>darkenergy</defName>
    <label>Contained Dark Energy</label>
    <description>A container holding the pure essence of dark energy, a mixture of science and magic. Used to make dark matter.</description>
    <graphicData>
      <texPath>Items/DarkEnergymkii</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <stackLimit>1</stackLimit>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <MarketValue>2000</MarketValue>
      <Mass>2</Mass>
      <Flammability>1</Flammability>
      <DeteriorationRate>2.0</DeteriorationRate>
    </statBases>
    <comps>
      <li Class="CompProperties_Explosive">
        <explosiveRadius>5</explosiveRadius>
        <explosiveDamageType>Bomb</explosiveDamageType>
        <explosiveExpandPerStackcount>0.4</explosiveExpandPerStackcount>
        <startWickHitPointsPercent>0.8</startWickHitPointsPercent>
        <wickTicks>
          <min>30</min>
          <max>60</max>
        </wickTicks>
      </li>
    </comps>
    <intricate>true</intricate>
    <thingCategories>
      <li>Manufactured</li>
    </thingCategories>
    <tradeTags>
      <li>ExoticMisc</li>
    </tradeTags>
  </ThingDef>
